class Math {
  static square(x) {
    return x * x;
  }
  static pi() {
    return 3.14;
  }
  static one_over_pi() {
    return 1 / this.pi();
  }
}

class Circle {
  init(radius) {
    this.radius = radius;
  }
  area() {
    return Math.pi() * Math.square(this.radius);
  }
}

